#!/usr/bin/expect -f
set timeout 30

set server [lindex $argv 0]
set user [lindex $argv 1]
set pass [lindex $argv 2]
set port [lindex $argv 3]

spawn ssh $user@$server -p $port

expect {
  "> " { }
  "$ " { }
  "assword: " { 
        send "$pass\n" 
  }
  "(yes/no)? " { 
        send "yes\n"
  }
  default {
        send_user "Login failed\n"
        exit
  }
}

interact